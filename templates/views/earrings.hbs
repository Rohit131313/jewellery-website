<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="icon" type="image/x-icon" href="/newlogo.png">
    <link rel="stylesheet" href="style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
    <title>Gallery</title>
    <style>
        .main {
            background: rgb(255,172,172);
background: linear-gradient(90deg, rgba(255,172,172,0.7596288515406162) 20%, rgba(167,247,247,0.9052871148459384) 99%);
            height: 550px;
            width: cover;
            padding-top: 1vw;

        }

        .topnav {
            overflow: hidden;
            background-color: #f2efef;

            margin-right: 12vw;
            margin-left: 12vw;
            border-radius: 2vw;
        }

        .logo {
            float: left;
            padding: 14px 16px;
            border-radius: 50%;
            height: 5vw;
            width: 5vw;
            margin-left: 1vw;
            margin-right: 1vw;
            margin-top: 0.5vw;
        }

        .topnav .abc {
            float: left;
            display: block;
            color: #121111;
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
            font-size: 25px;
            margin-top: 0.5vw;
        }

        .active1 {
            background-color: rgb(228, 26, 164);
            color: white;
            width: fit-content;
            height: fit-content;
            border-radius: 1vw;
        }

        .active {
            background-color: #06a691;
            color: white;
        }

        .topnav .icon {
            display: none;
        }

        .dropdown {
            float: left;
            overflow: hidden;
        }

        .dropdown .dropbtn {
            font-size: 17px;
            border: none;
            outline: none;
            color: white;
            padding: 14px 16px;
            background-color: inherit;
            font-family: inherit;
            margin: 0;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #f9f9f9;
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
            z-index: 1;
        }

        .dropdown-content a {
            float: none;
            color: black;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
            text-align: left;
        }

        .topnav a:hover,
        .dropdown:hover .dropbtn {
            background-color: #555;
            color: white;
        }

        .dropdown-content a:hover {
            background-color: #ddd;
            color: black;
        }

        .dropdown:hover .dropdown-content {
            display: block;
        }

        @media screen and (max-width: 1081px) {

            .topnav a:not(:first-child),
            .dropdown .dropbtn {
                display: none;
            }

            .topnav a.icon {
                float: right;
                display: block;
            }
        }

        @media screen and (max-width: 1081px) {
            .topnav.responsive {
                position: relative;
            }

            .topnav.responsive .icon {
                position: absolute;
                right: 0;
                top: 0;
            }

            .topnav.responsive a {
                float: none;
                display: block;
                text-align: left;
            }

            .topnav.responsive .dropdown {
                float: none;
            }

            .topnav.responsive .dropdown-content {
                position: relative;
            }

            .topnav.responsive .dropdown .dropbtn {
                display: block;
                width: 100%;
                text-align: left;
            }


        }

        .grid {
            display: grid;
            grid-template-rows: 1fr 1fr 1fr 1fr 1fr;
            border: 1px solid black;
            float: left;
            height: fit-content;
            padding: 2vw;
            width: fit-content;
            background-color: #06a691;
            margin-top: 7vw;
            margin-left: 2vw;
            position: fixed;
            border-radius: 2vw;
        }

        .grid-items {


            color: black;
            font-size: 2vw;
            font-weight: bold;
        }

        .photo-grid {
            display: grid;
            grid-template-columns: repeat(2, 25vw);
            grid-template-rows: repeat(4, 25vw);
            gap: 1vw;
            margin-left: 40%;
            margin-top: 10%;
        }

        .photo-grid img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 2vw;
            border: 1px solid black;
        }

        .heading {
            color: black;
            background-color: rgb(9, 227, 198);
            margin-left: 41%;
            margin-top: 5%;
            margin-right: 15%;
            padding-left: 3%;
            border-radius: 1vw;
            font-size: 4vw;
        }

        .last {
            padding-left:10vw;
            height: 25vw;
            display: grid;
            grid-template-columns: 1fr 1fr 1fr;
            background: rgb(255,172,172);
background: linear-gradient(90deg, rgba(255,172,172,0.7596288515406162) 20%, rgba(167,247,247,0.9052871148459384) 99%);
        }

        .logo1 {
            height: 6vw;
            width: 6vw;
            margin-left: 21vw;
            margin-top: 4vw;

        }

        .plogo1 {
            margin-top: 1vw;
            margin-left: 9vw;
            font-size: 1vw;
        }

        .second1 {
            display: inline-block;
        }

        .second {
            margin-top: 3vw;
            margin-left: 9vw;
        }

        .third {
            margin-top: 5vw;
        }

        .footer {
            background-color: grey;
            color: white;
            padding-left: 40%;
            font-weight: bold;

        }
        @media screen and (max-width: 1081px) {
            .second {
                margin-top: 0vw;
            }
            .last{
                padding-left: 0vw;
                height: 30vw;
                display: grid;
                grid-template-columns: 1fr 1fr 1fr;
                background: rgb(214, 241, 65);
                background: linear-gradient(90deg, rgba(214, 241, 65, 1) 0%, rgba(233, 223, 11, 1) 35%, rgba(6, 166, 145, 1) 100%)
            }
        }
    </style>
</head>

<body>
    <div class="main">

        <header>
    <div class="topnav" id="myTopnav">
        <img class="logo" src="/displayimage/newlogo.png">
        <a href="/index "class=" abc">Home</a>
        <a href="/rings" class="active abc">Gallery</a>
        <a href="/about" class="abc">About</a>
        <a href="/contact" class="abc">Contact</a>
        <a href="https://wa.me/919709706575"><i id=wts class="fa fa-whatsapp abc" style="font-size:50px;color:rgb(33, 240, 10);margin-top:1.5vw;float:right;margin-right:2vw;"></i></a>
        <a href="javascript:void(0);" style="font-size:25px;" class="icon" onclick="myFunction()">&#9776;</a>
    </div>
    
</header>
        {{>gallerynavbar}}
        
        <h1 class="heading">Earrings Collection</h1>
        <div class="photo-grid">
            {{#each x}}
            <img src="./uploadearrings/{{this.Picture}}">
            {{/each}}
            {{!-- <img src="e1.png" alt="Photo 1">
            <img src="e2.png" alt="Photo 2">
            <img src="e3.png" alt="Photo 3">
            <img src="e4.png" alt="Photo 4">
            <img src="e5.png" alt="Photo 5">
            <img src="e6.png" alt="Photo 6">
            <img src="e7.png" alt="Photo 7">
            <img src="e8.png" alt="Photo 8"> --}}
        </div>
        {{>footer}}

</body>
<script>
    function myFunction() {
    var x = document.getElementById("myTopnav");
    if (x.className === "topnav") {
      x.className += " responsive";
    } else {
      x.className = "topnav";
    }
  }

    let wts = document.getElementById("wts");
    url = "https://api.whatsapp.com/send/?phone=9709706575&text&type=phone_number&app_absent=0";
